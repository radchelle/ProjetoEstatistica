        "    'ASIAN/PACIFIC ISLANDER',\n",
        "    'BLACK',\n",
        "    'BLACK HISPANIC',\n",
        "    'WHITE',\n",
        "    'WHITE HISPANIC',\n",
        "    'UNKNOWN/REFUSED'\n",
        "]\n",
        "\n",
        "tabela_genero_area = pd.crosstab(\n",
        "    criminosos_nas_areas['GENDER'],\n",
        "    criminosos_nas_areas['RACE'],\n",
        "    normalize='index'\n",
        ") * 100\n",
        "for col in ordem_colunas:\n",
        "    if col not in tabela_genero_area.columns:\n",
        "        tabela_genero_area[col] = 0.0\n",
